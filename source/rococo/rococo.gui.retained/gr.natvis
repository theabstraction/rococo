<?xml version="1.0" encoding="utf-8"?> 
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
	<Type Name="GRANON::GRPanel">
		<DisplayString>{{{ desc } #{uniqueId}. {absRect}. Flags: {flags}. Parent {parent->desc}. Grandparent { parent->parent->desc}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRCollapser">
		<DisplayString>{{Collapser}: { panel.desc }. Button {collapseButton}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRDivision">
		<DisplayString>{{Division}: { panel.desc }}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRVerticalList">
		<DisplayString>{{VerticalList: { panel.desc }}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRMainFrame">
		<DisplayString>{{"GRMainFrame": {panel.desc }}</DisplayString>
	</Type>
	<Type Name="GRANON::GRToolbar">
		<DisplayString>{{Toolbar: { panel.desc }}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRTable">
		<DisplayString>{{Table: { panel.desc }, cols {columnHeaders}, rows {rows}}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRButton">
		<DisplayString>{{Btn: Title:{title}, Image: {imagePath}, iMeta{iMetaData}, sMeta{sMetaData}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRSlider">
		<DisplayString>{{Slider}}</DisplayString>
	</Type>
	<Type Name="GRANON::GRMenuBar">
		<DisplayString>{{Menu. { panel.desc }}</DisplayString>
	</Type>
	<Type Name="GRANON::GRSplitter">
		<DisplayString>{{Splitter. { panel.desc } L {first}, R {second}</DisplayString>
	</Type>
	<Type Name="Rococo::Gui::CursorEvent">
		<DisplayString>{{Pos {position}. Click {click}. Id #{eventId}}}</DisplayString>
	</Type>
	<Type Name="Rococo::Gui::CursorClick">
		<DisplayString>{{LD {LeftButtonDown}, LU {LeftButtonUp}, RD {RightButtonDown}, RU {RightButtonUp}, MD {MidButtonDown}, MU {MidButtonUp}, Wheel {MouseWheel}, Unk {Unknown}}}</DisplayString>
	</Type>
	<Type Name="Rococo::Gui::GRAnchors">
		<DisplayString>{{Anchors L: {left}, R: {right}, T: {top}, B: {bottom}, XH: {expandsHorizontally}, XV: {expandsVertically}}}</DisplayString>
	</Type>
	<Type Name="Rococo::Gui::GRAnchorPadding">
		<DisplayString>{{Padding L: {left}, R: {right}, T: {top}, B: {bottom}}}</DisplayString>
	</Type>
</AutoVisualizer>