(namespace EntryPoint)

(function Main -> (Int32 exitValue1):
	(debug)
    (exitValue1 = (MyFunction1 5000))
)

(function MyFunction1 (Int32 x) -> (Int32 result):
    (if (x > 100)
		(result = (MyFunction2 x)) 
	else 
		(result = x)
	)
)

(function MyFunction2 (Int32 x) -> (Int32 result):
   (if (x > 100) 
		(result = (MyFunction1 (x / 2)))
	else
		(result = x)
	)
)
(alias Main EntryPoint.Main)