(interface Sys.IDog
	(Bark -> )
)

(class Dog (implements Sys.IDog)	
)

(method Dog.Bark -> :
	(Sys.Print "Woof")
)

(factory Sys.NewDog Sys.IDog :
	(construct Dog)
)

(method Dog.Construct :
)

(function CreateNewDog -> (Sys.IDog dog):
	(Sys.IDog newDog (Sys.NewDog))
	(dog = newDog)
	(yield)
)

(namespace EntryPoint)
	(alias CreateNewDog EntryPoint.CreateObject)

