(' #import (mhost "MHost"))

(namespace EntryPoint)
	(alias Main EntryPoint.Main)
	
(using Sys.Maths)
(using Sys.Type)
(using MPlat)
(using MHost)
(using MHost.OS)
(using Rococo)
(using Rococo.Entities)
(using Rococo.Graphics)
(using Rococo.Audio)


(function Main (Int32 id) -> (Int32 exitCode):
	(ISpriteBuilder sprites (SpriteBuilder))
	(sprites.AddSprite "!textures/test/AI-castle.jpg")
	(sprites.AddEachSpriteInDirectory "!textures/toolbars")
	(sprites.AddEachSpriteInDirectory "!textures/prompts/xbox")
	(sprites.AddEachSpriteInDirectory "!textures/prompts/keyboard")
	(sprites.AddEachSpriteInDirectory "!textures/toolbars")
	(sprites.AddEachSpriteInDirectory "!textures/toolbars/3rd-party/www.aha-soft.com")
	(sprites.LoadAllSprites)
			
	(IEngine engine (Engine))
	(engine.SetNextScript "!scripts/mhost/great.sex.screen.sxy")
)