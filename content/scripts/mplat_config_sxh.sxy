(interface MPlat.IConfig
	(Int (Sys.Type.IString name)(Int32 value) -> )
	(Float (Sys.Type.IString name)(Float32 value) -> )
	(Bool (Sys.Type.IString name)(Bool value) -> )
	(Text (Sys.Type.IString name)(Sys.Type.IString value) -> )
	(GetInt (Sys.Type.IString name) -> (Int32 value))
	(GetFloat (Sys.Type.IString name) -> (Float32 value))
	(GetBool (Sys.Type.IString name) -> (Bool value))
	(GetText (Sys.Type.IString name)(Sys.Type.IStringBuilder text) -> )
)

(class ProxyIConfig (implements MPlat.IConfig) (Pointer hObject) (attribute not-serialized))
	(method ProxyIConfig.Construct (Pointer hObject): (this.hObject = hObject))
	(method ProxyIConfig.Int (Sys.Type.IString name)(Int32 value) ->  : (MPlat.Native.IConfigInt this.hObject name value))
	(method ProxyIConfig.Float (Sys.Type.IString name)(Float32 value) ->  : (MPlat.Native.IConfigFloat this.hObject name value))
	(method ProxyIConfig.Bool (Sys.Type.IString name)(Bool value) ->  : (MPlat.Native.IConfigBool this.hObject name value))
	(method ProxyIConfig.Text (Sys.Type.IString name)(Sys.Type.IString value) ->  : (MPlat.Native.IConfigText this.hObject name value))
	(method ProxyIConfig.GetInt (Sys.Type.IString name) -> (Int32 value) : (MPlat.Native.IConfigGetInt this.hObject name -> value))
	(method ProxyIConfig.GetFloat (Sys.Type.IString name) -> (Float32 value) : (MPlat.Native.IConfigGetFloat this.hObject name -> value))
	(method ProxyIConfig.GetBool (Sys.Type.IString name) -> (Bool value) : (MPlat.Native.IConfigGetBool this.hObject name -> value))
	(method ProxyIConfig.GetText (Sys.Type.IString name)(Sys.Type.IStringBuilder text) ->  : (MPlat.Native.IConfigGetText this.hObject name text))

(factory MPlat.Config MPlat.IConfig  :
	(Pointer pObject = (MPlat.Native.GetHandleForIConfig0 ))
	(construct ProxyIConfig pObject)
)

