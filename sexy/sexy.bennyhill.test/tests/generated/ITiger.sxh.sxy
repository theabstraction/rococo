(interface Sys.Animals.ITiger
	(Write (Sys.Type.IString text) -> )
)

(class ProxyITiger (implements Sys.Animals.ITiger) (Pointer hObject))
	(method ProxyITiger.Construct (Pointer hObject): (this.hObject = hObject))
	(ProxyITiger.Destruct -> : (Sys.Animals.Native.ITigerDestruct this.hObject))
	(method ProxyITiger.Write (Sys.Type.IString text) ->  : (Sys.Animals.Native.ITigerWrite this.hObject text))

(factory Sys.Animals.GetTigerByName Sys.Animals.ITiger (Sys.Type.IString tigerName) :
	(Pointer pObject = (Sys.Animals.Native.GetHandleForITiger0 tigerName))
	(construct ProxyITiger pObject)
)

