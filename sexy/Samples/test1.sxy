(namespace EntryPoint)
(alias Main EntryPoint.Main)

(using Sys.Type)

(struct Mouse
	(Int32 x)
	(Int32 y)
	(IString name)
)

(function SetMouseName (Mouse m) -> :
	(m.name = "Geoff")
)

(function Main -> (Int32 result):
	(Mouse m)
	(SetMouseName m)
	(Sys.Print m.name -> result)
)
