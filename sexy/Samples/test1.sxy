(using Sys.Type)

(namespace EntryPoint)
 (alias Main EntryPoint.Main)

(function Main -> (Int32 result):			
  (Lapdancer lapdancer)
  (lapdancer.pole.Index -> result)
)

(interface Sys.IPole
  (Index -> (Int32 value))
)

(class Pole
  (implements Sys.IPole)
  (Int32 index)
)

(method Pole.Index -> (Int32 index): (index = this.index))

(struct Lapdancer
  (Sys.IPole pole)
)