(namespace EntryPoint)
(alias Main EntryPoint.Main)

(using Sys.Type)

(struct Button
	(Int32 state)
    (IString name)
)

(struct Mouse
	(Button b1)
	(Button b2)
    (IString name)
)

(function Main (Int32 id) -> (Int32 result) :
	(Mouse m = (0 "left") (1 "right") "Helen")
	(Sys.Print m.name -> result)
)