(namespace EntryPoint)
  (alias Main EntryPoint.Main)
(using Sys.Reflection)
(using Sys.Type)

(using Sys.Maths)

(struct Mat2x2i
	(Vec2i row1 row2)
)

(function Main -> (Int32 result):
	(Mat2x2i m = (4 3)(2 1))
	(Float32 x = (5 - (m.row1.x - m.row2.x)))
	(if (x == 3) (result = 12))
)