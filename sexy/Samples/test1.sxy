(namespace EntryPoint)
	(alias Main EntryPoint.Main)

(interface Sys.IRobot)
(interface Sys.ICat)
(class RobotCat (implements Sys.IRobot)(implements Sys.ICat))
(method RobotCat.Construct : )
(factory Sys.NewRobotCat Sys.IRobot : (construct RobotCat))

(function Main -> (Int32 result):
		(Sys.IRobot bot (Sys.NewRobotCat))
		(cast bot -> Sys.ICat cat)
		(if (bot != cat) (Sys.Throw 0 "Bad cat"))
		(if (not (bot == cat)) (Sys.Throw 0 "Badder cat"))
)