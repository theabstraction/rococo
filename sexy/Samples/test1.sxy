(namespace EntryPoint)
 (alias Main EntryPoint.Main)

(using Sys.Type)

(function Main -> (Int32 result):

	(list Int32 a)

	(a.Append 17)
	(a.Append 34)
	(a.Append 333)

	(foreach n # a 
		(try
			(
				(Sys.Throw n.Value "thrown in foreach")
			)
		catch ex
			(
				(result += 1)
				(Sys.Print ex.Message)
			)
		)
		(result += 10)
	)
)