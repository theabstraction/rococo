(namespace EntryPoint)
	(alias Main EntryPoint.Main)

(using Sys.Type)

(function BuildArray (array Int32 a)-> :
	(Int32 i = 0)
	(while (i < 10)
		(a.Push (i + 10))
		(i += 1)
	)
)

(function SumElements (array Int32 a)-> (Int32 sum):
	(foreach i k # a
		(sum += k)
	)	
)


(function Main -> (Int32 result):
	(array Int32 a (10) )
	
	(BuildArray a)

	(result = (SumElements a))
)
